#############
### Debug ###
#############
config {
    no_cache = 0
    compressJs = 1
    compressCss = 1
    concatenateJs = 1
    concatenateCss = 1
    admPanel = 0
    sendCacheHeaders = 1
}
[applicationContext = Development]
    config {
        no_cache = 0
        compressJs = 0
        compressCss = 0
        concatenateJs = 0
        concatenateCss = 0
        admPanel = 0
        sendCacheHeaders = 0
    }
[global]

############
### PAGE ###
############
page {
    logo {
        file = EXT:fotoblog_sitepackage/Resources/Public/Images/logo.svg
        fileInverted = EXT:fotoblog_sitepackage/Resources/Public/Images/logo-inverted.svg
        height = 52
        width = 192
        alt = Fotoblog Sitepackage
        linktitle = Fotoblog Sitepackage
    }
    favicon {
        file = EXT:fotoblog_sitepackage/Resources/Public/Icons/favicon.ico
    }
    fluidtemplate {
        layoutRootPath = EXT:fotoblog_sitepackage/Resources/Private/Layouts/Page/
        partialRootPath = EXT:fotoblog_sitepackage/Resources/Private/Partials/Page/
        templateRootPath = EXT:fotoblog_sitepackage/Resources/Private/Templates/Page/
    }
}



##################
### EXTENSIONS ###
##################
plugin.bootstrap_package {
    view {
        layoutRootPath = EXT:fotoblog_sitepackage/Resources/Private/Layouts/
        partialRootPath = EXT:fotoblog_sitepackage/Resources/Private/Partials/
        templateRootPath = EXT:fotoblog_sitepackage/Resources/Private/Templates/
    }
}




########################
### CONTENT ELEMENTS ###
########################
plugin.bootstrap_package_contentelements {
    view {
        layoutRootPath = EXT:fotoblog_sitepackage/Resources/Private/Layouts/ContentElements/
        partialRootPath = EXT:fotoblog_sitepackage/Resources/Private/Partials/ContentElements/
        templateRootPath = EXT:fotoblog_sitepackage/Resources/Private/Templates/ContentElements/
    }
}


################################
### BOOTSTRAP SCSS CONSTANTS ###
################################

plugin.bootstrap_package.settings.scss {
    primary = #D03F38
    secondary = #333333
}
