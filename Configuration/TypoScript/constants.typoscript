######################
#### DEPENDENCIES ####
######################
@import "EXT:bootstrap_package/Configuration/TypoScript/constants.typoscript"
@import "EXT:skom_sitepackage/Configuration/TypoScript/constants.typoscript"
@import "EXT:blog/Configuration/TypoScript/Integration/constants.typoscript"
@import "EXT:fotoblog_sitepackage/Configuration/TypoScript/ContentElement/constants.typoscript"

##################
### EXTENSIONS ###
##################
@import "EXT:fotoblog_sitepackage/Configuration/TypoScript/Extension/RxShariff/constants.typoscript"
@import "EXT:fotoblog_sitepackage/Configuration/TypoScript/Extension/Blog/constants.typoscript"
@import "EXT:fotoblog_sitepackage/Configuration/TypoScript/Extension/infinitescrolling/constants.typoscript"


############
### PAGE ###
############
page {
    logo {
        file = EXT:fotoblog_sitepackage/Resources/Public/Images/logo.svg
        fileInverted = EXT:fotoblog_sitepackage/Resources/Public/Images/logo-inverted.svg
        height = 52
        width = 192
        alt = Fotoblog Sitepackage
        linktitle = Fotoblog Sitepackage
    }
    favicon {
        file = EXT:fotoblog_sitepackage/Resources/Public/Icons/favicon.ico
    }
    fluidtemplate {
        layoutRootPath = EXT:fotoblog_sitepackage/Resources/Private/Layouts/Page/
        partialRootPath = EXT:fotoblog_sitepackage/Resources/Private/Partials/Page/
        templateRootPath = EXT:fotoblog_sitepackage/Resources/Private/Templates/Page/
    }
}







################################
### BOOTSTRAP SCSS CONSTANTS ###
################################

plugin.bootstrap_package.settings.scss {
    primary = #D03F38
    secondary = #333333
}
