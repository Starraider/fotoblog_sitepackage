plugin.tx_cookieman.settings.trackingObjects {
    GoogleAnalytics {
        # inline scripts would break Content-Security-Policy: script-src
        # https://developer.mozilla.org/en-US/docs/Web/HTTP/Headers/Content-Security-Policy/script-src
#        inject (
#            <script data-what="GoogleAnalytics" src="/typo3conf/ext/fotoblog_sitepackage/Resources/Public/JavaScript/GoogleAnalytics-inject.js"></script>
#        )
        # example:
        inject (
<script async src="https://www.googletagmanager.com/gtag/js?id=UA-189024-9"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'UA-189024-9');
</script>
        )
        show {
            # https://developers.google.com/analytics/devguides/collection/gtagjs/cookie-usage
            _ga {
                duration = 2
                durationUnit = years
                type = cookie_http+html
                provider = Google
            }

            _gat {
                duration = 1
                durationUnit = day
                type = cookie_http+html
                provider = Google
            }

            _gid {
                duration = 1
                durationUnit = day
                type = cookie_http+html
                provider = Google
            }

            _ga_--container-id-- {
                duration = 2
                durationUnit = years
                type = cookie_http+html
                provider = Google
            }

            _gac_--property-id-- {
                duration = 3
                durationUnit = months
                type = cookie_http+html
                provider = Google
            }
        }
    }
}
