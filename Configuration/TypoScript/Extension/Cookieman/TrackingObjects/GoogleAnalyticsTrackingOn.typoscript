plugin.tx_cookieman.settings.trackingObjects {
    GoogleAnalyticsTrackingOn {
        inject (
<script>console.log('Google Analytics inserted with Tracking');</script>
        )
        show {
            # https://developers.google.com/analytics/devguides/collection/gtagjs/cookie-usage
            _ga {
                duration = 2
                durationUnit = years
                type = cookie_http+html
                provider = Google
            }

            _gat {
                duration = 1
                durationUnit = day
                type = cookie_http+html
                provider = Google
            }

            _gid {
                duration = 1
                durationUnit = day
                type = cookie_http+html
                provider = Google
            }

            _ga_--container-id-- {
                duration = 2
                durationUnit = years
                type = cookie_http+html
                provider = Google
            }

            _gac_--property-id-- {
                duration = 3
                durationUnit = months
                type = cookie_http+html
                provider = Google
            }
        }
    }
}
